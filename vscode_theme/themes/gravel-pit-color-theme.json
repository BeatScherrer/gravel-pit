{
   "colors": {
      "activityBarBadge.background": "#007acc",
      "editor.background": "#ff0000",
      "editor.foreground": "#eeffff",
      "sideBarTitle.foreground": "#bbbbbb",
      "terminal.ansiBlack": "#000000",
      "terminal.ansiBlue": "#6699cc",
      "terminal.ansiBrightBlack": "#4e4e4e",
      "terminal.ansiBrightBlue": "",
      "terminal.ansiBrightCyan": "#56fff1",
      "terminal.ansiBrightGreen": "#85f8a2",
      "terminal.ansiBrightMagenta": "",
      "terminal.ansiBrightRed": "",
      "terminal.ansiBrightWhite": "#eeffff",
      "terminal.ansiBrightYellow": "#ffeac3",
      "terminal.ansiCyan": "#56fff1",
      "terminal.ansiGreen": "#589c69",
      "terminal.ansiMagenta": "#d477ff",
      "terminal.ansiRed": "#ff5e5e",
      "terminal.ansiWhite": "#bbbbbb",
      "terminal.ansiYellow": "#ffeac3",
      "terminal.background": "#252525",
      "terminal.foreground": "#bbbbbb",
      "terminal.selectionBackground": "#ff0000"
   },
   "name": "gravel-pit",
   "tokenColors": [
      {
         "name": "Comment",
         "scope": [
            "comment",
            "punctuation.definition.comment"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#8a8a8a"
         }
      },
      {
         "name": "String",
         "scope": [
            "string"
         ],
         "settings": {
            "foreground": "#56fff1"
         }
      },
      {
         "name": "Punctuation",
         "scope": [
            "punctuation.section",
            "punctuation.terminator.statement",
            "punctuation.definition.parameters",
            "punctuation.definition.dictionary",
            "punctuation.separator.dot-access",
            "punctuation.separator.pointer-access"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Terminator statement",
         "scope": [
            "punctuation.terminator.statement"
         ],
         "settings": {
            "foreground": "#bbbbbb"
         }
      },
      {
         "name": "Operators",
         "scope": [
            "keyword.operator"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "keywords",
         "scope": [
            "storage.type",
            "support.type.property-name.json.comments",
            "markup.heading"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Storage modifier",
         "scope": [
            "storage.modifier",
            "storage.type.modifier"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Preprocessor",
         "scope": [
            "meta.preprocessor",
            "keyword.control.directive.conditional",
            "keyword.control.directive.define",
            "keyword.control.directive.include",
            "keyword.other.using.directive.cpp",
            "markup.list.unnumbered"
         ],
         "settings": {
            "foreground": "#ff5e5e"
         }
      },
      {
         "name": "test",
         "scope": [
            "entity.name.function.preprocessor",
            "meta.preprocessor.macro"
         ],
         "settings": {
            "fontStyle": "bold",
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "includes",
         "scope": [
            "string.quoted.other.lt-gt.include.cpp",
            "string.quoted.double.include"
         ],
         "settings": {
            "foreground": "#56fff1"
         }
      },
      {
         "name": "Function Call",
         "scope": [
            "entity.name.function.call",
            "entity.name.function.member",
            "markup.italic"
         ],
         "settings": {
            "foreground": "#6699cc"
         }
      },
      {
         "name": "Methods",
         "scope": [
            "entity.name.function.definition",
            "markup.bold"
         ],
         "settings": {
            "foreground": "#589c69"
         }
      },
      {
         "name": "Function argument",
         "scope": [
            "variable.parameter"
         ],
         "settings": {
            "foreground": "#ff9035"
         }
      },
      {
         "name": "Class and Struct",
         "scope": [
            "entity.name.type.class",
            "entity.name.type.struct",
            "entity.name.function.definition.special.constructor"
         ],
         "settings": {
            "foreground": "#589c69"
         }
      },
      {
         "name": "Member variables",
         "scope": [
            "variable.other.property.cpp",
            "variable.other.object.access.cpp"
         ],
         "settings": {
            "foreground": "#85f8a2"
         }
      },
      {
         "name": "Class Access Modifiers",
         "scope": [
            "storage.type.modifier",
            "storage.modifier.specifier.functional"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Inherited",
         "scope": [
            "entity.name.type.inherited"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#85f8a2"
         }
      },
      {
         "name": "Namespace",
         "scope": [
            "entity.name.namespace",
            "entity.name.scope-resolution",
            "punctuation.separator.namespace.access"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "variables",
         "scope": [
            "variable"
         ],
         "settings": {
            "foreground": "#eeffff"
         }
      },
      {
         "name": "Control Keywords",
         "scope": [
            "keyword.control",
            "constant.language.NULL"
         ],
         "settings": {
            "foreground": "#ff5e5e"
         }
      },
      {
         "name": "Template",
         "scope": [
            "storage.type.template"
         ],
         "settings": {
            "foreground": "#d477ff"
         }
      },
      {
         "name": "test",
         "scope": [ ],
         "settings": {
            "foreground": "#ffffff"
         }
      }
   ],
   "type": "dark"
}
