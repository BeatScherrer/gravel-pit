      "editor.background": "#000000",
      "editor.foreground": "#eeffff",
      "gitDecoration.addedResourceForeground": "",
      "gitDecoration.conflictingResourceForeground": "#d477ff",
      "gitDecoration.deletedResourceForeground": "#FF5E5E",
      "gitDecoration.ignoredResourceForeground": "#4e4e4e",
      "gitDecoration.modifiedResourceForeground": "#ff9035",
      "gitDecoration.untrackedResourceForeground": "#4e4e4e",
      "minimap.errorHighlight": "#FF5E5E",
      "minimap.findMatchHighlight": "#ffeac3",
      "minimap.selectionHighlight": "#56fff1",
      "minimap.warningHighlight": "#ff9035",
      "minimapGutter.addedBackground": "",
      "minimapGutter.deletedBackground": "#FF5E5E",
      "minimapGutter.modifiedBackground": "#ffeac3",
      "sideBarTitle.foreground": "",
      "terminal.ansiBlack": "#000000",
      "terminal.ansiBlue": "#007acc",
      "terminal.ansiBrightBlack": "#555555",
      "terminal.ansiBrightBlue": "",
      "terminal.ansiBrightCyan": "#56fff1",
      "terminal.ansiBrightGreen": "",
      "terminal.ansiBrightMagenta": "",
      "terminal.ansiBrightRed": "",
      "terminal.ansiBrightWhite": "",
      "terminal.ansiBrightYellow": "#ffeac3",
      "terminal.ansiCyan": "#56fff1",
      "terminal.ansiGreen": "",
      "terminal.ansiMagenta": "#d477ff",
      "terminal.ansiRed": "#FF5E5E",
      "terminal.ansiWhite": "",
      "terminal.ansiYellow": "#ffeac3"
   },
   "name": "gravel-pit",
   "tokenColors": [
      {
         "name": "Comment",
         "scope": [
            "comment",
            "punctuation.definition.comment"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": "#4e4e4e"
         }
      },
      {
         "name": "String",
         "scope": [
            "string"
         ],
         "settings": {
            "foreground": "#56fff1"
         }
      },
      {
         "name": "Punctuation",
         "scope": [
            "punctuation.section",
            "punctuation.terminator.statement",
            "punctuation.definition.parameters",
            "punctuation.definition.dictionary"
         ],
         "settings": {
            "foreground": ""
         }
      },
      {
         "name": "Terminator statement",
         "scope": [
            "punctuation.terminator.statement"
         ],
         "settings": {
            "foreground": ""
         }
      },
      {
         "name": "Operators",
         "scope": [
            "keyword.operator"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "keywords",
         "scope": [
            "storage.type",
            "support.type.property-name.json.comments",
            "markup.heading"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Storage modifier",
         "scope": [
            "storage.modifier",
            "storage.type.modifier"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Preprocessor",
         "scope": [
            "meta.preprocessor",
            "keyword.control.directive.conditional",
            "keyword.control.directive.define",
            "keyword.control.directive.include",
            "markup.list.unnumbered"
         ],
         "settings": {
            "foreground": "#FF5E5E"
         }
      },
      {
         "name": "test",
         "scope": [
            "entity.name.function.preprocessor",
            "meta.preprocessor.macro"
         ],
         "settings": {
            "fontStyle": "bold",
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "includes",
         "scope": [
            "string.quoted.other.lt-gt.include.cpp"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Function Declarations",
         "scope": [
            "meta.function",
            "markup.italic"
         ],
         "settings": {
            "foreground": "#007acc"
         }
      },
      {
         "name": "Function Call",
         "scope": [
            "entity.name.function.call"
         ],
         "settings": {
            "foreground": "#007acc"
         }
      },
      {
         "name": "Methods",
         "scope": [
            "entity.name.function.member",
            "markup.bold"
         ],
         "settings": {
            "foreground": ""
         }
      },
      {
         "name": "Function argument",
         "scope": [
            "variable.parameter"
         ],
         "settings": {
            "foreground": "#ff9035"
         }
      },
      {
         "name": "Class and Struct",
         "scope": [
            "entity.name.type.class",
            "entity.name.type.struct"
         ],
         "settings": {
            "foreground": ""
         }
      },
      {
         "name": "Class Access Modifiers",
         "scope": [
            "storage.type.modifier",
            "storage.modifier.specifier.functional"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "Special Methods",
         "scope": [
            "entity.name.function.constructor",
            "entity.name.function.destructor"
         ],
         "settings": {
            "foreground": ""
         }
      },
      {
         "name": "Inherited",
         "scope": [
            "entity.name.type.inherited"
         ],
         "settings": {
            "fontStyle": "italic",
            "foreground": ""
         }
      },
      {
         "name": "Namespace",
         "scope": [
            "entity.name.namespace",
            "entity.name.scope-resolution",
            "punctuation.separator.namespace.access"
         ],
         "settings": {
            "foreground": "#ffeac3"
         }
      },
      {
         "name": "variables",
         "scope": [
            "variable"
         ],
         "settings": {
            "foreground": ""
         }
      },
      {
         "name": "Control Keywords",
         "scope": [
            "keyword.control"
         ],
         "settings": {
            "foreground": "#FF5E5E"
         }
      },
      {
         "name": "Template",
         "scope": [
            "storage.type.template"
         ],
         "settings": {
            "foreground": "#d477ff"
         }
      },
      {
         "name": "test",
         "scope": [ ],
         "settings": {
            "foreground": "#ffffff"
         }
      }
   ],
   "type": "dark"
}
