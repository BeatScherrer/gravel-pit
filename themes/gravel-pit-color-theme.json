{
	"name": "gravel-pit",
	"type": "dark",
	"colors": {
		"editor.background": "#252525",
		"editor.foreground": "#eeffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#4e4e4e"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#56fff1"
			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation.section",
				"punctuation.terminator.statement",
				"punctuation.definition.parameters",

				"punctuation.definition.dictionary"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Terminator statement",
			"scope": [
				"punctuation.terminator.statement"
			],
			"settings": {
				"foreground": "#777777",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"foreground": "#ffeac3"
			}
		},
		{
			"name": "keywords",
			"scope": [
				"storage.type",
				"support.type.property-name.json.comments"
			],
			"settings": {
				"foreground": "#ffeac3"
			}
		},
		{
			"name": "Storage modifier",
			"scope": [
				"storage.modifier",
				"storage.type.modifier"
			],
			"settings": {
				"foreground": "#ffeac3"
			}
		},
		{
			"name": "Preprocessor",
			"scope": [
				"meta.preprocessor",
				"keyword.control.directive.conditional",
				"keyword.control.directive.define",
				"keyword.control.directive.include"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "test",
			"scope": [
				"entity.name.function.preprocessor",
				"meta.preprocessor.macro"
			],
			"settings": {
				"foreground": "#ffeac3",
				"fontStyle": "bold"
			}
		},
		{
			"name": "includes",
			"scope": [
				"string.quoted.other.lt-gt.include.cpp"
			],
			"settings": {
				"foreground": "#ffeac3"
			}
		},
		{
			"name": "Function Declarations",
			"scope": [
				"meta.function"
			],
			"settings": {
				"foreground": "#6699CC"
			}
		},
		{
			"name": "Function Call",
			"scope": [
				"entity.name.function.call"
			],
			"settings": {
				"foreground": "#6699CC"
			}
		},
		{
			"name": "Methods",
			"scope": [
				"entity.name.function.member"
			],
			"settings": {
				"foreground": "#7fbfff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function argument",
			"scope": [
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ff9035"
			}
		},	
		{
			"name": "Class and Struct",
			"scope" : [
				"entity.name.type.class",
				"entity.name.type.struct"
			],
			"settings": {
				"foreground": "#85ffa4"
			}
		},
		{
			"name": "Class Access Modifiers",
			"scope": [
				"storage"
			],
			"settings": {
				"foreground": "#a06464"
			}
		},
		{
			"name": "Special Methods",
			"scope": [
				"entity.name.function.constructor",
				"entity.name.function.destructor"
			],
			"settings": {
				"foreground": "#85ffa4"
			}
		},
		{
			"name": "Inherited",
			"scope": [

			],
			"settings": {
				"foreground": "#FF0000",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Namespace",
			"scope": [
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"punctuation.separator.namespace.access"
			],
			"settings": {
				"foreground": "#ffeac3"
			}
		},
		{
			"name": "variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#00ddad"
			}
		},	
		{
			"name": "Control Keywords",
			"scope": [
				"keyword.control"
			],
			"settings": {
				"foreground": "#FF5E5E"
			}
		},
		{
			"name": "test",
			"scope": [
			],
			"settings": {
				"foreground": "#FF0000",
				"fontStyle": "bold"
			}
		}
	]
}
