{
  "name": "gravel-pit",
  "type": "dark",
  "colors": {
    "editor.background": "#252525",
    "editor.foreground": "#eeffff",
    "activityBarBadge.background": "#007acc",
    "sideBarTitle.foreground": "#bbbbbb"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment"],
      "settings": {
        "foreground": "#4e4e4e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string"],
      "settings": {
        "foreground": "#56fff1"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function"],
      "settings": {
        "foreground": "#6699cc"
      }
    },
    {
      "name": "Function Arguments",
      "scope": [
        // General
        "variable.parameter",
        // Markup
        "punctuation.definition.list.begin.markdown",
        "punctuation.definition.bold",
        "punctuation.definition.italic",
        "markup.italic",
        "markup.bold",
        // HTML
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ffa155"
      }
    },
    {
      "name": "class 1",
      "scope": [
        // General
        "storage",
        "entity.name.scope-resolution",
        "keyword.operator",
        // HTML
        "punctuation.definition.tag",
        "support.type.property-name",
        // TS
        "meta.object-literal.key.ts"
      ],
      "settings": {
        "foreground": "#ffeac3"
      }
    },
    {
      "name": "class 2",
      "scope": ["entity.name.type.class", "variable.language.this"],
      "settings": {
        "foreground": "#85f8a2"
      }
    },
    {
      "name": "member variables",
      "scope": [
        "variable.other.property",
        // JS
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#85f8a2"
      }
    },
    {
      "name": "Preprocessor",
      "scope": [
        // General
        "keyword",
        // Markup
        "markup.heading",
        // HTML
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#FF5e5e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Template",
      "scope": ["storage.type.template", "markup.inline"],
      "settings": {
        "foreground": "#d477ff"
      }
    }
  ]
